<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qwe.yunifang">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/winning_bottle"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="design_width"
            android:value="768" />
        <meta-data
            android:name="design_height"
            android:value="1280" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lanuch.LanuchActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".lanuch.LocateActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ProductShowActivity" />
        <activity android:name=".DescriptionActivity" />
        <activity android:name=".ProductDetailsActivity"></activity>
    </application>

</manifest>